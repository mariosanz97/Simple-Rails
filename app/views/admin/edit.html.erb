<div class="edit-form-father">
  <h1 class="title">Edit</h1>
  <%= form_for @admin_edit, url: {controller: "admin", action: "update"}, :method => :patch do |f| %>
  <%= f.text_field :user, :placeholder => 'User' %>
  <%= f.text_field :password, :placeholder => 'Password'%>
  <%= f.submit 'Update', :class => 'btn btn-warning btn btn-primary' %>
  <% end %>
</div>